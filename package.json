{
  "name": "poly2020",
  "version": "0.1.0",
  "main": "index.js",
  "type": "module",
  "devDependencies": {
    "@types/node": "12.12.53",
    "@typescript-eslint/eslint-plugin": "~3.7.0",
    "dts-gen": "0.6.0",
    "flamebearer": "1.1.3",
    "stackvis": "0.5.0",
    "tap": "14.10.8",
    "typescript": "3.9.7"
  },
  "preferGlobal": true,
  "bin": {
    "poly2020": "out/src/cli/poly.js"
  },  
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rgrannell1/poly2020.git"
  },
  "bugs": {
    "url": "https://github.com/rgrannell1/poly2020/issues"
  },  
  "scripts": {
    "tsc": "tsc",
    "perf:clean": "rm isolate* || echo 'no files to remove'",
    "perf:measure": "npm run tsc && npm run perf:clean && node --prof out/src/cli/poly.js --config config.json --name profile",
    "perf:show": "npm run perf:measure && node --prof-process --preprocess -j isolate*.log | node_modules/.bin/flamebearer"
  },
  "author": "Róisín Grannell <r.grannell2@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@types/chai": "4.2.12",
    "@types/deepmerge": "2.2.0",
    "@types/docopt": "0.6.31",
    "@types/merge-stream": "1.1.2",
    "@types/signale": "1.4.1",
    "chai": "4.2.0",
    "deepmerge": "4.2.2",
    "docopt": "0.6.2",
    "durand-kerner": "1.0.0",
    "iter-tools": "6.2.6",
    "lzma-native": "6.0.1",
    "merge-stream": "2.0.0",
    "save-pixels": "2.3.4",
    "signale": "1.4.0",
    "zeros": "1.0.0"
  }
